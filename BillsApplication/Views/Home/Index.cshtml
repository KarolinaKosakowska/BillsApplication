@using Microsoft.AspNetCore.Identity
<center>
    @inject SignInManager<IdentityUser> SignInManager
    @inject UserManager<IdentityUser> UserManager
    @if (!SignInManager.IsSignedIn(User))
    {
        <div class="app-items__container">
            <div class="app-item">

                <a class="app-link" asp-area="Identity" asp-page="/Account/Register">Register</a>
                <a class="app-link" asp-area="Identity" asp-page="/Account/Login">Login</a>
            </div>
        </div>
    }
</center>
